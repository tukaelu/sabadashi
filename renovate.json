{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:weekends",
    ":disableDependencyDashboard"
  ],
  "timezone": "Asia/Tokyo",
  "semanticCommits": "enabled",
  "postUpdateOptions":[
    "gomodTidy"
  ],
  "commitMessagePrefix": "chore(all): ",
  "commitMessageAction": "update",
  "labels": ["renovate"],
  "rangeStrategy": "bump",
  "vulnerabilityAlerts": {
    "labels": ["security"]
  }
}
